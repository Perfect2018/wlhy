.container>view{
  height: 100%;
  z-index: 1;
}
.container>view>view{
  display: flex;
  justify-content: center;
  align-items: center;
  flex-wrap: wrap;
  margin-top: 29rpx;
  box-sizing: border-box
  /* height: 100% */
}
.input{
  display: block;
  width: 687rpx;
  height: 110rpx;
  background: #ffffff;
  box-shadow:0px 2px 2px 0px rgba(117,117,117,0.09);
  border-radius:20rpx;
  padding: 32rpx;
  margin-bottom: 25rpx;
  box-sizing: border-box
}
.place{
  font-size: 32rpx;
  color: #999999
}
.container>view>view{
  font-size: 28rpx;
  color: #999999;
  text-align: center;
}
.container>view>view.bottom{
  position: fixed;
  bottom: 38rpx;
  margin: 0 auto;
  width: 100%;
}
.btnBottom{
  margin: 0
}
.container>view>.fail{
  width: 566rpx;
  height: 369rpx;
  background: #ffffff;
  border-radius: 20rpx;
  display: flex;
  justify-content: space-around;
  flex-wrap: wrap;
  align-items: center
}
.container>view>view.fail>view:first-child{
  width: 100%;
  height: 28rpx;
  display: flex;
  justify-content: flex-end
}
.fail .close{
  width: 28rpx;
  height: 28rpx;
}
.container>view>.fail>view:nth-child(2){
  width: 100%;
  font-size: 40rpx;
  color: #000000;
  display: flex;
  align-items: center;
  justify-content: center
}
.container>view>.fail>view:nth-child(3){
  font-size: 28rpx;
  color: #666666
}
view.mask{
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 10;
  background: rgba(0,0,0,0.4);
  display: flex;
  justify-content: center;
  align-items: center;
  flex-wrap: wrap
}
view.fail image{
  width: 68rpx;
  height: 68rpx;
  margin-right: 20rpx;
}
view.fail button{
  width: 313rpx;
  height: 80rpx;
  background: #ffffff;
  border: 2rpx solid #15af7d;
  border-radius: 40rpx;
  color: #15af7d;
  font-size: 36rpx;
  display: flex;
  align-items: center;
  justify-content: center
}

