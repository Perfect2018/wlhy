.container>view{
  display: flex;
  justify-content: space-around;
  flex-wrap: wrap;
  padding-top: 10rpx;
  box-sizing: border-box
}
.container>view>view{
  margin-top: 20rpx;
  width: 687rpx;
  height: 390rpx;
  background: #ffffff;
  border-radius: 20rpx;
  box-shadow:0px 2px 2px 0px rgba(117,117,117,0.09)
}
.container .bottom{
  position: fixed;
  bottom: 38rpx;
  margin-bottom: 0;
}
view.tip{
  display: flex;
  justify-content: center;
  align-items: center;
}
view.tip>view{
  width: 50%;
  /* height: 100%; */
  display: flex;
  justify-content: center;
  align-items: center;
  flex-wrap: wrap
}
.mini{
  width: 121rpx;
  height: 121rpx;
  margin-bottom: 36rpx;
}
view.tip>view.Idcard{
  width: 486rpx;
  height: 306rpx;
  /* background: red; */
  border-radius: 20rpx;
}
view.Idcard>image{
  width: 100%;
  height: 100%;
}