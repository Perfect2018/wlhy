.userInfo{
  width: 687rpx;
  height: 205rpx;
  border-radius: 20rpx;
  background: #ffffff;
  box-shadow:0px 2px 2px 0px rgba(117,117,117,0.09);
  display: flex;
  justify-content: flex-start;
  align-items: center;
  margin: 30rpx 32rpx 32rpx 32rpx;
}

.avatar{
  width: 132rpx;
  height: 132rpx;
  border-radius: 50%;
  margin-left: 35rpx;
  margin-right: 27rpx;
}

.otherInfo{
  width: 687rpx;
  height: 205rpx;
  border-radius: 20rpx;
  background: #ffffff;
  box-shadow:0px 2px 2px 0px rgba(117,117,117,0.09);
  margin-left: 32rpx;
  margin-right: 32rpx;
  /* padding-left: 32rpx;
  padding-right: 32rpx; */
  box-sizing: border-box;
}

.otherInfo view{
  /* width: 100%; */
  height: 102rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  /* margin: 0rpx 33rpx; */
  box-sizing: border-box;
}

.otherInfo>view:first-child{
  border-bottom: 1rpx solid #e3e3e3;
}

.goViewInfo{
  padding-left: 32rpx;
  padding-right: 32rpx
}
.lookContract{
  padding-left: 32rpx;
  padding-right: 32rpx
}
.icon{
  width: 28rpx;
  height: 32rpx;
  margin-right: 18rpx;
  margin-top: 4rpx;
}

.detail{
  width: 12rpx;
  height: 20rpx;
}
.contract{
  font-size: 32rpx;
  color: #111111
}
.complain{
  width: 687rpx;
  height: 100rpx;
  line-height: 100rpx;
  border-radius: 20rpx;
  background: #ffffff;
  box-shadow:0px 2px 2px 0px rgba(117,117,117,0.09);
  margin-left: 32rpx;
  margin-right: 32rpx;
  margin-top: 32rpx;
  padding-left: 32rpx;
  padding-right: 32rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center
}